{
  "version": 2,
  "name": "gateway",
  "port": 8080,
  "timeout": "10s",
  "cache_ttl": "3600s",
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [],
      "expose_headers": [
        "Content-Length",
        "Content-Type",
        "authorization",
        "Location"
      ],
      "allow_headers": [
        "Content-Type",
        "Origin",
        "X-Requested-With",
        "Accept",
        "authorization",
        "secret",
        "Host"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  },
  "endpoints": [
    {
      "endpoint": "blog/oauth2/code/{code}",
      "method": "GET",
      "backend": [
        {
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/oauth2/code/{code}"
        }
      ],
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "blog/post/id/{id}",
      "method": "GET",
      "backend": [
        {
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/post/id/{id}"
        }
      ],
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "blog/post/count",
      "method": "GET",
      "backend": [
        {
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/post/count"
        }
      ],
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "blog/post/count/tag/{tag}",
      "method": "GET",
      "backend": [
        {
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/post/count/tag/{tag}"
        }
      ],
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "blog/post/page/{page}/size/{size}",
      "method": "GET",
      "backend": [
        {
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/post/page/{page}/size/{size}"
        }
      ],
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "blog/post/page/{page}/size/{size}/tag/{tag}",
      "method": "GET",
      "backend": [
        {
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/post/page/{page}/size/{size}/tag/{tag}"
        }
      ],
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "blog/comment/save",
      "method": "POST",
      "backend": [
        {
          "encoding": "json",
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/comment/save"
        }
      ],
      "headers_to_pass": [
        "Content-Length",
        "Content-Type"
      ],
      "concurrent_calls": 1,
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "blog/comment/id/{id}/login/{login}",
      "method": "DELETE",
      "backend": [
        {
          "host": [
            "http://host.docker.internal:9998"
          ],
          "url_pattern": "/comment/id/{id}/login/{login}"
        }
      ],
      "concurrent_calls": 1,
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "admin/user/login",
      "method": "POST",
      "backend": [
        {
          "encoding": "json",
          "host": [
            "http://host.docker.internal:9999"
          ],
          "url_pattern": "/admin/user/login"
        }
      ],
      "headers_to_pass": [
        "Content-Length",
        "Content-Type"
      ],
      "concurrent_calls": 1,
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "admin/user/tokenvalid",
      "method": "GET",
      "backend": [
        {
          "encoding": "json",
          "host": [
            "http://host.docker.internal:9999"
          ],
          "url_pattern": "/admin/user/tokenvalid"
        }
