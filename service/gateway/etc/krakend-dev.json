          "host": [
            "http://host.docker.internal:9999"
          ],
          "url_pattern": "/admin/countinfo"
        }
      ],
      "concurrent_calls": 1,
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "admin/post/page/{page}/size/{size}",
      "method": "GET",
      "backend": [
        {
          "encoding": "json",
          "host": [
            "http://host.docker.internal:9999"
          ],
          "url_pattern": "/admin/post/page/{page}/size/{size}"
        }
      ],
      "concurrent_calls": 1,
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "admin/post/count",
      "method": "GET",
      "backend": [
        {
          "encoding": "json",
          "host": [
            "http://host.docker.internal:9999"
          ],
          "url_pattern": "/admin/post/count"
        }
      ],
      "concurrent_calls": 1,
