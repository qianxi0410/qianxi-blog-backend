        "Content-Length",
        "Content-Type",
        "authorization"
      ],
      "concurrent_calls": 1,
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "admin/comment/id/{id}/postid/{postid}",
      "method": "DELETE",
      "backend": [
        {
          "encoding": "json",
          "host": [
            "http://host.docker.internal:9999"
          ],
          "url_pattern": "/admin/comment/id/{id}/postid/{postid}"
        }
      ],
      "headers_to_pass": [
        "authorization"
      ],
      "concurrent_calls": 1,
      "timeout": "10s",
      "cache_ttl": 3600
    },
    {
      "endpoint": "admin/system/update",
      "method": "POST",
      "backend": [
        {
          "encoding": "json",
          "host": [
            "http://host.docker.internal:9999"
          ],
